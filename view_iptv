FAB_IPTV_SW = 
var DataHoje = TODAY()
var DataFabrica = MAX('IPTV_SW'[FABRICA])
var AtrasoFabrica = MAX('IPTV_SW'[ATRASO_FABRICA])
var AprovadoFOA = MAX('IPTV_SW'[APROVADO_FOA])
var ValidacaoEx = MAX('IPTV_SW'[VALIDACAO_EXECUTIVA_PILOTO])
var AprovadoHomolog = MAX('IPTV_SW'[APROVADO_HOMOLOG])
var AprovadoFUT = MAX('IPTV_SW'[APROVADO_FUT])
var DataInicioScripts = MAX('IPTV_SW'[INICIO_SCRIPTS_MASSIFICACAO])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoFabrica = 2, UPPER(FORMAT(DataFabrica, "DDMMM")) & "  " & "ðŸŸ¡", //estÃ¡ em atraso
    DataFabrica >= DataHoje && DataHoje < DataInicioScripts, UPPER(FORMAT(DataFabrica, "DDMMM")) & "  " & "ðŸ”µ", // fÃ¡brica estÃ¡ em andamento
    DataFabrica <= DataHoje && DataFabrica <> BLANK(), UPPER(FORMAT(DataFabrica, "DDMMM")) & "  " & "ðŸŸ¢", // fÃ¡brica concluÃ­do
    DataFabrica > DataHoje, UPPER(FORMAT(DataFabrica, "DDMMM")) & "  " & "âšª", // Ã© uma data futura
    ISBLANK(DataFabrica), "-" // OPCH indefinido
    )
    return RESULTADO

---------------------------------------------------------------------------------------------

FOA_IPTV_SW = 
var DataHoje = TODAY()
var DataInicioFOA = MAX('IPTV_SW'[INICIO_FOA])
var DataTerminoFOA = MAX('IPTV_SW'[TERMINO_FOA])
var AtrasoIFOA = MAX('IPTV_SW'[ATRASO_FOA])
var AprovadoFOA = MAX('IPTV_SW'[APROVADO_FOA])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoIFOA = 1, UPPER(FORMAT(DataInicioFOA, "DDMMM")) & "  " & "ðŸŸ¡", //estÃ¡ em atraso
    AprovadoFOA = 0 && AprovadoFOA <> BLANK(), UPPER(FORMAT(DataInicioFOA, "DDMMM")) & "  " & "ðŸ”´", //FOA reprovado
    DataInicioFOA <= DataHoje && DataHoje <= DataTerminoFOA, UPPER(FORMAT(DataInicioFOA, "DDMMM")) & "  " & "ðŸ”µ", // homologaÃ§Ã£o estÃ¡ em andamento
    DataTerminoFOA < DataHoje && DataTerminoFOA <> BLANK(), UPPER(FORMAT(DataInicioFOA, "DDMMM")) & "  " & "ðŸŸ¢", // homologaÃ§Ã£o concluÃ­da
    DataInicioFOA > DataHoje, UPPER(FORMAT(DataInicioFOA, "DDMMM")) & "  " & "âšª", // Ã© uma data futura
    ISBLANK(DataInicioFOA), "-" // se inicio FOA nÃ£o for preenchido ou for reprovado
    )
    return RESULTADO

---------------------------------------------------------------------------------------------

FUT_IPTV_SW = 
var DataHoje = TODAY()
var DataInicioFUT = MAX('IPTV_SW'[INICIO_FUT])
var DataTerminoFUT = MAX('IPTV_SW'[TERMINO_FUT])
var AtrasoFUT = MAX('IPTV_SW'[ATRASO_FUT])
var AprovadoFUT = MAX('IPTV_SW'[APROVADO_FUT])


OPCH_IPTV_SW = 
var DataHoje = TODAY()
var DataOPCH = MAX('IPTV_SW'[OPCH_PROD])
var AtrasoOPCH = MAX('IPTV_SW'[ATRASO_OPCH_PROD])
var DataFabrica = MAX('IPTV_SW'[FABRICA])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoOPCH = 1, UPPER(FORMAT(DataOPCH, "DDMMM")) & "  " & "ðŸŸ¡", //estÃ¡ em atraso
    DataOPCH >= DataHoje && DataHoje < DataFabrica, UPPER(FORMAT(DataOPCH, "DDMMM")) & "  " & "ðŸ”µ", // OPCH estÃ¡ em andamento
    DataFabrica <= DataHoje && DataFabrica <> BLANK(), UPPER(FORMAT(DataOPCH, "DDMMM")) & "  " & "ðŸŸ¢", // OPCH concluÃ­do
    DataOPCH > DataHoje, UPPER(FORMAT(DataOPCH, "DDMMM")) & "  " & "âšª", // Ã© uma data futura
    ISBLANK(DataOPCH), "-" // OPCH indefinido
    )
    return RESULTADO


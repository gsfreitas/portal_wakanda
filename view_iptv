HOMOLOG_IPTV = 
var DataHoje = TODAY()
var DataInicioHomolog = MAX('IPTV_SW'[INICIO_HOMOLOG])
var DataTerminoHomolog = MAX('IPTV_SW'[TERMINO_HOMOLOG])
var AtrasoHomolog = MAX('IPTV_SW'[ATRASO_HOMOLOG])
var AprovadoHomolog = MAX('IPTV_SW'[APROVADO_HOMOLOG])
var ValidaEx = MAX('IPTV_SW'[VALIDACAO_EXECUTIVA_PILOTO])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoHomolog = 1, UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸŸ¡", //estÃ¡ em atraso
    (AprovadoHomolog = 0 && AprovadoHomolog <> BLANK()) || (ValidaEx = 0 && ValidaEx <> BLANK()), UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸ”´", //estÃ¡ reprovado
    DataInicioHomolog <= DataHoje && DataHoje <= DataTerminoHomolog, UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸ”µ", // homologaÃ§Ã£o estÃ¡ em andamento
    DataTerminoHomolog < DataHoje && DataTerminoHomolog <> BLANK(), UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸŸ¢", // FUT concluÃ­do
    DataInicioHomolog > DataHoje, UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "âšª", // Ã© uma data futura
    ISBLANK(DataInicioHomolog), "-" // se termino FUT nÃ£o for preenchido
    )
    return RESULTADO


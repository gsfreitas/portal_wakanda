HOMOLOG_IPTV_HW = 
var DataHoje = TODAY()
var AtrasoHomolog = MAX('IPTV_HW'[ATRASO_HOMOLOG])
var DataInicioHomolog = MAX('IPTV_HW'[INICIO_HOMOLOG])
var DataTerminoHomolog = MAX('IPTV_HW'[TERMINO_HOMOLOG])
var AprovadoHomolog = MAX('IPTV_HW'[APROVADO_HOMOLOG])
var ValidacaoEx = MAX('IPTV_HW'[VALIDACAO_EXECUTIVA_PILOTO])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoHomolog = 2 && AtrasoHomolog <> BLANK(), UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸŸ¡", // homologaÃ§Ã£o estÃ¡ em atraso
    (AprovadoHomolog = 1 && AprovadoHomolog <> BLANK()) || (ValidacaoEx = 2 && ValidacaoEx <> BLANK()), UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸ”´", // estÃ¡ reprovado
    DataInicioHomolog <= DataHoje && DataHoje < DataTerminoHomolog, UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸ”µ", // homologaÃ§Ã£o estÃ¡ em andamento
    DataTerminoHomolog <= DataHoje && DataTerminoHomolog <> BLANK(), UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "ðŸŸ¢", // homologaÃ§Ã£o concluÃ­da
    DataInicioHomolog > DataHoje, UPPER(FORMAT(DataInicioHomolog, "DDMMM")) & "  " & "âšª" // Ã© uma data futura
     //se nÃ£o for preenchido
    )
    return RESULTADO


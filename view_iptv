SCRIPTS_IPTV_SW = 
var DataHoje = TODAY()
var DataInicioScripts = MAX('IPTV_SW'[INICIO_SCRIPTS_MASSIFICACAO])
var DataTerminoScripts = MAX('IPTV_SW'[TERMINO_SCRIPTS_MASSIFICACAO])
var AtrasoScripts = MAX('IPTV_SW'[ATRASO_MASSIFCACAO])

var RESULTADO = 
    SWITCH(TRUE(),
    AtrasoScripts = 2, UPPER(FORMAT(DataInicioScripts, "DDMMM")) & "  " & "ðŸŸ¡", //estÃ¡ em atraso
    (DataInicioScripts <= DataHoje) && (DataHoje < DataTerminoScripts), UPPER(FORMAT(DataInicioScripts, "DDMMM")) & "  " & "ðŸ”µ", // OPCH estÃ¡ em andamento
    DataTerminoScripts <= DataHoje && DataTerminoScripts <> BLANK(), UPPER(FORMAT(DataInicioScripts, "DDMMM")) & "  " & "ðŸŸ¢", // OPCH concluÃ­do
    DataInicioScripts > DataHoje, UPPER(FORMAT(DataInicioScripts, "DDMMM")) & "  " & "âšª", // Ã© uma data futura
    ISBLANK(DataInicioScripts), "-" // Scripts indefinido
    )
    return RESULTADO

---------------------------------------------------------------------------------------------

SW_IPTV = 
var DataHoje = TODAY()
var AtrasoSW = MAX('IPTV_SW'[ATRASO_SW])
var DataSW = MAX('IPTV_SW'[CHEGADA_SW])
var NovoSW = MAX('IPTV_SW'[NOVO_SW])

var RESULTADO = 
    SWITCH(TRUE(),
    ISBLANK(DataSW), "-",
    AtrasoSW = 1, UPPER(FORMAT(DataSW, "DDMMM")) & "  " & "ðŸŸ¡", //a chegada do SW estÃ¡ atrasada
        DataSW <= DataHoje && DataSW <> DATE(1990,01,01), UPPER(FORMAT(DataSW, "DDMMM")) & "  " & "ðŸŸ¢", //JÃ¡ estamos com o FW
        DataSW = DATE(1990,01,01), "-", //NÃ£o chegou o FW ainda, por isso nÃ£o foi preenchido
        DataSW > DataHoje, UPPER(FORMAT(DataSW, "DDMMM")) & "  " & "âšª" //Data futura
    )
    return RESULTADO

---------------------------------------------------------------------------------------------

